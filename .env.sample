# Jenkins System Configuration
JENKINS_ADMIN_EMAIL=
JENKINS_URL=
JENKINS_JAVA_OPTS=-Xmx256m -Xms256m -XX:MaxMetaspaceSize=128m -XX:+UseG1GC -XX:+ExitOnOutOfMemoryError -Djava.awt.headless=true -Xlog:gc*=debug:/var/log/jenkins/gc.log
JENKINS_OPTS=--handlerCountMax=100 --logfile=/var/log/jenkins/jenkins.log

# Resource Configuration
JENKINS_MEMORY_LIMIT=500m
JENKINS_NUM_EXECUTORS=2
JENKINS_QUIET_PERIOD=5

# SMTP Configuration
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_DEFAULT_RECIPIENTS=
MAILER_SMTP_PASSWORD=

# SSH Keys and Environment Files
NG_BACKEND_SSH_KEY=
NG_TESTING_SSH_KEY=
NG_BACKEND_ENV=
NG_TESTING_ENV=

# Admin User Configuration
JENKINS_ADMIN_USER=
JENKINS_ADMIN_NAME=
JENKINS_ADMIN_EMAIL=

# Additional System Configuration
JENKINS_SYSTEM_MESSAGE="Jenkins optimized for low memory usage"
JENKINS_CRUMB_ISSUER_EXCLUDE_CLIENT=false
JENKINS_ENABLE_SLAVE_AGENT_PORT=true
JENKINS_SLAVE_AGENT_PORT=50000

# Build Configuration
JENKINS_BUILD_DISCARDER_DAYS=7
JENKINS_BUILD_DISCARDER_NUM=5
JENKINS_ARTIFACT_DAYS=7
JENKINS_ARTIFACT_NUM=3

# Git Configuration
JENKINS_GIT_USER_NAME=
JENKINS_GIT_USER_HOME="git"
